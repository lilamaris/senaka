{
  "servers": [
    {
      "id": "lmstudio-local",
      "provider": "openai-compatible",
      "baseUrlEnv": "OPENAI_BASE_URL",
      "apiKeyEnv": "OPENAI_API_KEY",
      "description": "Local LM Studio OpenAI-compatible endpoint"
    }
  ],
  "models": [
    {
      "id": "main-default",
      "serverId": "lmstudio-local",
      "model": "glm-4.7-flash@q4_1",
      "temperature": 0.2,
      "description": "Primary synthesis model"
    },
    {
      "id": "worker-fast",
      "serverId": "lmstudio-local",
      "model": "glm-4.7-flash@q4_1",
      "temperature": 0.1,
      "extraBody": {
        "think": false
      },
      "description": "Fast worker model with think disabled"
    }
  ],
  "agents": {
    "default": {
      "mode": "main-worker",
      "mainModelId": "main-default",
      "workerModelId": "worker-fast",
      "maxSteps": 3,
      "stream": true,
      "description": "Default agent profile"
    },
    "single": {
      "mode": "single-main",
      "mainModelId": "main-default",
      "maxSteps": 2,
      "stream": true,
      "description": "Single model fallback profile"
    }
  }
}
